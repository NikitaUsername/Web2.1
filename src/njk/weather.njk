<div id="DATA">
    <div class="row">
        <div class="col-lg-3">
            <img id="weatherPic" {{ information.weather[0].icon }}>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            <div id="city">City:</div>
        </div>
        <div class="col-lg-3">
            <div id="city1">{{ information.name }}</div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            <div id="temp">Temperature:</div>
        </div>
        <div class="col-lg-3">
            <div id="temp1">{{ information.main.temp }}
                ÂºC</div>
        </div>
    </div>
    <div class="row">
